{
  "name": "chuchy4ever/taxonomy-editor",
  "description": "An extension to add a GUI to edit taxonomies.",
  "type": "bolt-extension",
  "keywords": [
    "taxonomy",
    "gui",
    "taxonomy editor",
    "taxonomy visual editor",
    "editor",
    "category",
    "categories",
    "tags"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Jan Chuchvalec",
      "email": "chuchy4ever@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "bolt/core": "^5"
  },
  "autoload": {
    "psr-4": {
      "Chuchy4ever\\TaxonomyEditor\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "entrypoint": "Chuchy4ever\\TaxonomyEditor\\Extension"
  },
  "config": {
    "allow-plugins": {
      "symfony/flex": false,
      "drupol/composer-packages": false
    }
  }
}
